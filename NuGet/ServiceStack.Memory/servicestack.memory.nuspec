<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>ServiceStack.Memory</id>
    <title>ServiceStack.Memory</title>
    <version>5.0.0</version> 
    <authors>ServiceStack</authors> 
    <owners>ServiceStack</owners>
    <summary>ServiceStack .NET Core Platform bindings for native System.Memory Span APIs</summary>
    <description>
      Install in .NET Core 2.1 App and call `NetCoreMemory.Configure()` on Startup 
      to have ServiceStack.Text delegate to use .NET Core App's native System.Memory Span APIs. 
    </description> 
    <projectUrl>https://github.com/ServiceStack/ServiceStack.Text</projectUrl>
    <licenseUrl>https://servicestack.net/terms</licenseUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <iconUrl>https://servicestack.net/img/logo-32.png</iconUrl>
    <tags>Span Memory JSON Text Serializer CSV JSV Fast</tags>
    <language>en-US</language>
    <copyright>ServiceStack and contributors</copyright>
    <dependencies>
      <group targetFramework="netcoreapp2.1">
        <dependency id="System.Runtime" version="[4.3.0, )" />
        <dependency id="Microsoft.CSharp" version="[4.4.0, )" />
        <dependency id="System.Memory" version="[4.5.1, )" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="lib\**\*.*" target="lib" />
    <file src="..\..\src\ServiceStack.Memory\**\*.cs" target="src" />
  </files>
</package>

